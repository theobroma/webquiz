    "marked": "^1.1.1",
    "next": "^6.1.2",
    "polished": "^2.0.3",
    "pretty-ms": "^3.2.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-countdown-now": "^1.3.0",
    "react-dom": "^16.4.1",
    "react-highlight": "^0.12.0",
    "react-highlight.js": "^1.0.7",
    "react-markdown": "^3.6.0",
    "react-prism": "^4.3.2",
    "react-redux": "^5.0.7",
