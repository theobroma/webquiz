{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "eqeqeq": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-shadow": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/require-default-props": 0,
    "react/destructuring-assignment": 0,
    "react/button-has-type": 0,
    "react/prop-types": 0
  },
  "plugins": ["babel", "jsx-a11y", "react"]
}
